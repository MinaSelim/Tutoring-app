{
   "name": "express-ts",
   "version": "1.0.0",
   "description": "",
   "main": "build/src/server.js",
   "scripts": {
      "build": "tsc",
      "prebuild":"node prebuild.js",
      "prebuild-unix": "rm -rf build",
      "prebuild-windows": "cleandir build/",
      "postbuild": "cpy universityInformation/classes/** build/universityInformation/classes",
      "prestart": "npm run build",
      "start": "node .",
      "test": "mocha -r ts-node/register './tests/**/*.test.ts'",
      "lint": "prettier --check . && eslint --cache . --ext .ts,.tsx,./src",
      "lint-fix": "prettier --write . && eslint . --ext .ts,.tsx,./src --cache --fix",
      "coverage": "nyc npm run test",
      "prepush": "npm run lint && npm run test"
   },
   "author": "",
   "license": "ISC",
   "dependencies": {
      "aws-sdk": "^2.784.0",
      "express": "^4.17.1",
      "express-session": "^1.17.1",
      "firebase-admin": "^9.2.0"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.0",
      "@types/chai": "^4.2.13",
      "@types/chai-http": "^4.2.0",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.8",
      "@types/express-session": "^1.17.0",
      "@types/mocha": "^8.0.3",
      "@types/node": "^14.11.2",
      "@types/sinon": "^9.0.8",
      "@types/supertest": "^2.0.10",
      "@typescript-eslint/eslint-plugin": "^4.6.0",
      "@typescript-eslint/parser": "^4.4.1",
      "chai": "^4.2.0",
      "chai-http": "^4.3.0",
      "clean-dir": "^1.0.3",
      "cpy-cli": "^3.1.1",
      "dotenv": "^8.2.0",
      "eslint": "^7.11.0",
      "eslint-config-prettier": "^6.15.0",
      "eslint-plugin-prettier": "^3.1.4",
      "firebase-functions": "^3.11.0",
      "firebase-functions-test": "^0.2.2",
      "husky": "^4.3.0",
      "lint-staged": "^10.5.0",
      "mocha": "^8.2.0",
      "nodemon": "^2.0.4",
      "nyc": "^15.1.0",
      "prettier": "^2.1.2",
      "sinon": "^9.2.0",
      "sinon-chai": "^3.5.0",
      "supertest": "^5.0.0",
      "ts-mocha": "^7.0.0",
      "ts-mockito": "^2.6.1",
      "ts-node": "^9.0.0",
      "ts-sinon": "^2.0.1",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^4.0.3"
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "lint-staged": {
      "*.{ts,ts,/src}": [
         "eslint --cache --fix",
         "prettier --write",
         "mocha -r ts-node/register './tests/**/*.test.ts'"
      ]
   },
   "nyc": {
      "skip-empty": true,
      "reporter": [
         "lcov"
      ],
      "report-dir": "./coverage",
      "all": true,
      "extensions": [
         ".ts"
      ],
      "include": [
         "src"
      ],
      "exclude": [
         "**/*.test.ts"
      ],
      "clean": true
   }
}
